<div class="container">
  <div class="row">
    <% @stores.each do |store| %>
    <div class="col s12 m6 offset-m3">
      <div class="card blue darken-3">
        <div class="card-content white-text">
          <h4>店名</h4>
          <span class="card-title"><%= store.store_name %></span>
          <h6>使用できるクレジットカード</h6>
          <p><%= store.c_card %></p>
          <h6>使用できる電子マネー</h6>
          <p><%= store.e_money %></p>
        </div>
        <div class="card-action">
          <a href="#">This is a link</a>
          <a href="#">This is a link</a>
        </div>
      </div>
    </div>
    <% end %>
  </div>
  <%= paginate(@stores) %>
</div>
